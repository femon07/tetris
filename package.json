{
  "name": "tetris",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://femon07.github.io/tetris",
  "description": "A modern Tetris game with clean architecture and testable design",
  "scripts": {
    "start": "webpack serve",
    "prebuild": "node scripts/prebuild.js",
    "build": "webpack",
    "test": "jest --verbose --no-cache",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist docs",
    "postbuild": "npm run copy-files",
    "copy-files": "cpx \"index.html\" dist && cpx \"dist/index.html\" docs && cpx \"dist/main.js\" docs && cpx \"dist/main.js\" ."
  },
  "keywords": [
    "tetris",
    "game",
    "javascript",
    "esmodules"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/runtime": "^7.27.6",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "rimraf": "^6.0.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true,
            "node": "current"
          },
          "useBuiltIns": "usage",
          "corejs": 3,
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "dependencies": {
    "core-js": "^3.43.0"
  }
}
